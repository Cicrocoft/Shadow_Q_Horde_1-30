        </If>
		<If Condition="Nav.RangeFromMeXYZ(-225.40794372559, -5097.0756835938, 49.325393676758) &lt; 1">
            <Lua>SetCVar("autoLootDefault", "1"); print("Autoloot On");</Lua>
            <SetEnableAutoLoot Value="true"/>

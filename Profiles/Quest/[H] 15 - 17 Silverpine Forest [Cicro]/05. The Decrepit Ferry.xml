		<!-- Blackspots -->
		<!-- Hotspots -->
		<AddHotspot X="703.234375" Y="613.90234375" Z="43.874622344971" Range="200" />		
		<While Condition="Data.GetItemCountByName('A Talking Head') == 0" >
			<Wait Value="100" />
		</While>
	</If>
